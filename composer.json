{
  "name": "cofee_shop/app",
  "description": "Weather application",
  "type": "project",
  "homepage": "https://github.com/FlaviusPintican/test_oop",
  "require": {
    "php": "^8.0",
    "monolog/monolog": "^2.3",
    "phpoffice/phpspreadsheet": "dev-NumberFormatMask-Helpers",
    "vlucas/phpdotenv": "^5.4",
    "webmozart/assert": "^1.10"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "extra": {
  },
  "config": {
    "preferred-install": "dist",
    "secure-http": false,
    "sort-packages": true,
    "optimize-autoloader": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {
    "format": "php-cs-fixer fix --allow-risky=yes"
  }
}
